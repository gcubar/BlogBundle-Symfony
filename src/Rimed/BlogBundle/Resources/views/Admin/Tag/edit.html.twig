{% extends "RimedBlogBundle::layout.html.twig" %}

{% block title %}Editando la etiqueta - {{ tag.name }}{% endblock %}

{% block content %}
<form action="{{ path('blog_admin_tag_edit', { 'id': tag.id }) }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}

    <input type="submit" value="Guardar cambios" />
</form>
<a href="{{ path('blog_admin_tag_index') }}">Volver al listado</a>
{% endblock %}
